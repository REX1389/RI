<Page x:Class="Gogogo.Pages.Menu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gogogo.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="950" d:DesignWidth="1150"
      Title="Menu">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Grid.Column="1" Grid.Row="0" Margin="50" FontSize="50" Click="Button7">7 x 7</Button>
        <Button Grid.Column="1" Grid.Row="1" Margin="50" FontSize="50" Click="Button9">9 x 9</Button>
        <Button Grid.Column="1" Grid.Row="2" Margin="50" FontSize="50" Click="Button13">13 x 13</Button>
        <Button Grid.Column="1" Grid.Row="3" Margin="50" FontSize="50" Click="Button19">19 x 19</Button>
    </Grid>
</Page>
